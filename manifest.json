{
    "manifest_version": 2,
  
    "name": "Getting started example",
    "description": "Does magic ML stuff.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": ["*://www.google.ca/*", "https://www.google.com/*"],
            "css": [],
            "js": ["myscript.js"]
        },
        {
            "matches": ["http://www.mahmoodmahmood.info/*"],
            "css": [],
            "js": ["mytestscript.js"]   
        }
    ],
    "permissions": [
      "activeTab",
      "storage"
    ]
  }